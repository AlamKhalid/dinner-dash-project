<%= form_with model: @category do |f| %>
  <div class="row">
    <div class="col-md-6 mb-3">
      <%= f.label :name %>
      <%= f.text_field :name, class: "form-control" %>
    </div>
    <div class="col-md-6 mb-3"></div>
    <div class="col-md-6 mb-3">
      <%= f.submit 'Submit', class: 'btn btn-dark'%>
    </div>
  </div>
<% end %>
<%= render 'common/back_btn' %>
